#include <iostream>
using namespace std;

int main(){

	for (int i = 70; i < 81; i += 1)
		cout << i << " ";

	cout << endl;

	for (int i = 100; i < 301; i += 20)
		cout << i << " ";

	cout << endl;

	for (int i = 25; i > -26; i -= 5)
		cout << i << " ";

	cout << endl;

	for (int i = 10000; i > 10; i /= 5)
		cout << i << " ";
}
