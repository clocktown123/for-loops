#include<iostream>
using namespace std;

int main() {
	for (int i = 0; i < 2; i++) {
		for (int j = 0; j < 3; j++) {
			cout << "*";

		}
		cout << endl; 
	}

	cout << " " << endl;

	for (int i = 0; i < 2; i++) {
		for (int j = 1; j < 6; j++) {
			cout << j;

		}
		cout << endl;
	}

	for (int i = 0; i < 6; i++) {
		for (int j = 1; j < i; j++) {
			cout << "*";

		}
		cout << endl;
	}

	
}
